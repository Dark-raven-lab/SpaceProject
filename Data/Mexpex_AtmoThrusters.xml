	<!-- BASIC Atmospheric -->
	<Definition xsi:type="MyObjectBuilder_ThrustDefinition"> <!-- LS Atmospheric Vanila-->
		<Id>
			<TypeId>Thrust</TypeId>
			<SubtypeId>LargeBlockSmallAtmosphericThrust</SubtypeId>
		</Id>
		<!-- Normal Keen Properties ... -->
		<ModExtensions>
                <ModComponents>
                    <GameLogicComponent Name="MwiComponent" />
                </ModComponents>
                <Group Name="ThrusterCompData">
                    <Text Name="ID" Value= "LargeBlockSmallAtmosphericThrust"/> <!--REQUIRED! No specific value required Debugging  -->
                    <Boolean Name="hideVanillaFlame" Value="true" /><!-- Hide Vanilla Flame-->

                    <Text Name="EmitterSetup" Value="
                    SHould be minimal setup for just normal particle smoke ;
                    Particle : LG_AtmosphericThrustSmall;
                    Type: PARTICLE;
                    AtmoOnly: FALSE ;
                    ThrustMin : 0.01;
                    ThrustMax : 1;
                    Size:1;
                    Offset:0;
                    VelocityScalingFrom: -1;
                    VelocityScalingTo: 2;
                    Dummies : thruster_flame_02;
                    "/>
                </Group>
            </ModExtensions>
	</Definition>
